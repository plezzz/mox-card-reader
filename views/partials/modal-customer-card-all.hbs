<div class="modal fade" id="viewMemberCards" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content bg-dark">
            <div class="modal-header">
                <h5 class="modal-title">Карти на потребителя</h5>
            </div>
            <div class="modal-body">
                <table class="table table-hover">
                    <thead>
                    <tr>
                        <th scope="col">Създаден на</th>
                        <th scope="col">Сериен номер</th>
                        <th scope="col">Име</th>
                        <th scope="col">Брой влизания</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each member.cards}}
                        <tr>
                            <td>{{dateFixed createdAt}}</td>
                            <th scope="row">{{serialNumber}}</th>
                            <th scope="row">{{cardName}}</th>
                            <th scope="row">{{enteriesCount}}</th>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>

        </div>
    </div>
</div>
